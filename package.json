{
  "name": "Artunis",
  "version": "0.2.5",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle:android": "node ./node_modules/react-native/local-cli/cli.js bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.js --bundle-output ios/Artunis/main.jsbundle --dev=false --verbose",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -scheme Artunis archivexcodebuild -scheme Artunis archive",
    "reinstall": "rm -rf node_modules; yarn cache clean; yarn install",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "android-release": "react-native run-android --variant=release",
    "prettier": "prettier --write `find src -name \"*.js\"`",
    "test:lint": "eslint src/",
    "test:flow": "flow",
    "test:flow-coverage": "flow-coverage-report -i 'src/**/*.js' -x '**/__tests__/**' -t html",
    "test:unit": "jest src/",
    "test:coveralls": "jest --coverage",
    "test": "yarn run test:lint && yarn run test:coveralls",
    "version": "support/version-ios.sh",
    "postinstall": "sh ./postinstall.sh",
    "update-snapshots": "yarn run test:unit -- -u"
  },
  "dependencies": {
    "bip39": "^2.5.0",
    "bufferutil": "^3.0.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.21",
    "prop-types": "^15.6.0",
    "react": "^16.3.0-alpha.1",
    "react-native": "0.54.3",
    "react-native-animatable": "^1.2.4",
    "react-native-camera": "^1.0.2",
    "react-native-device-info": "^0.16.0",
    "react-native-keychain": "^2.0.0",
    "react-native-navigation": "^1.1.426",
    "react-native-notifications": "^1.2.0",
    "react-native-root-siblings": "^3.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-loop-symbol-ponyfill": "^2.2.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "ripple-bip32": "0.0.3",
    "ripple-keypairs": "^0.10.1",
    "ripple-lib": "^0.17.9",
    "ripple-sign-keypairs": "1.0.2",
    "sjcl": "^1.0.7",
    "socket.io": "^2.0.4",
    "text-encoding": "^0.6.4",
    "tweetnacl": "^1.0.0",
    "utf-8-validate": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-rewrite-require": "^1.14.5",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "babel-preset-react-native": "^4.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "eslint-plugin-spellcheck": "0.0.6",
    "flow-bin": "^0.65.0",
    "flow-coverage-report": "^0.5.0",
    "flow-typed": "^2.4.0",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "jest-react-native": "^18.0.0",
    "node-libs-browser": "^2.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "react-native-cli": "^2.0.1",
    "react-test-renderer": "^16.3.0-alpha.1",
    "remote-redux-devtools": "^0.5.12"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-native-root-siblings)"
    ],
    "collectCoverage": true,
    "verbose": true
  }
}
